{
    "name": "mc-server-management",
    "version": "3.1.0",
    "description": "A library for the Minecraft server management protocol added in 25w35a",
    "keywords": [
        "minecraft",
        "server",
        "management",
        "protocol"
    ],
    "license": "MIT",
    "author": "Julian Vennen <julian@aternos.org>",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.node.d.mts",
                "default": "./dist/index.node.mjs"
            },
            "require": {
                "types": "./dist/index.node.d.cts",
                "default": "./dist/index.node.cjs"
            },
            "browser": {
                "import": {
                    "types": "./dist/index.browser.d.mts",
                    "default": "./dist/index.browser.mjs"
                },
                "require": {
                    "types": "./dist/index.browser.d.cts",
                    "default": "./dist/index.browser.cjs"
                }
            }
        },
        "./package.json": "./package.json"
    },
    "browser": {
        "./dist/index.index.cjs": "./dist/index.browser.cjs",
        "./dist/index.d.cts": "./dist/index.browser.d.cts",
        "./dist/index.mjs": "./dist/index.browser.mjs",
        "./dist/index.d.mts": "./dist/index.browser.d.mts"
    },
    "main": "./dist/index.node.cjs",
    "module": "./dist/index.node.mjs",
    "types": "./dist/index.node.d.mts",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aternosorg/mc-server-management.git"
    },
    "scripts": {
        "spec": "./server.sh spec",
        "server:run": "./server.sh run",
        "server:start": "./server.sh run -d",
        "server:stop": "./server.sh stop",
        "server:logs": "./server.sh logs",
        "check": "tsc",
        "test": "vitest",
        "build": "tsdown"
    },
    "dependencies": {
        "eventemitter3": "^5.0.1",
        "rpc-websockets": "github:JulianVennen/rpc-websockets#protocols",
        "semver": "^7.7.3"
    },
    "devDependencies": {
        "@arethetypeswrong/core": "^0.18.2",
        "@types/semver": "^7.7.1",
        "@types/ws": "^8.18.1",
        "@vitest/browser-playwright": "^4.0.4",
        "@vitest/coverage-v8": "^4.0.4",
        "tsdown": "^0.15.0",
        "typescript": "^5.9.2",
        "unplugin-unused": "^0.5.3",
        "vitest": "^4.0.4"
    },
    "overrides": {
        "vite": "npm:rolldown-vite@latest"
    },
    "engines": {
        "node": ">=22.0.0"
    }
}
